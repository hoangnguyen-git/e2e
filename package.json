{
  "name": "common-automation-framework",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p ./",
    "lint": "tslint  --project tsconfig.json --config tslint.json",
    "lint:fix": "npm run lint --fix",
    "validate": "npm-run-all --parallel build lint",
    "postinstall": "webdriver-manager update --versions.chrome 99.0.4844.51 --gecko=true",
    "clean": "rm -rf auto-generated coverage dist e2e-report *.log",
    "clean:all": "npm run clean -- node_modules",
    "e2e": "protractor protractor.conf.js",
    "e2e-selenium": "protractor protractor.selenium-hub.conf.js",
    "e2e-bs": "protractor protractor.browserstack.conf.js",
    "posttest": "allure generate auto-generated/allure-results --clean -o auto-generated/allure-report || true"
  },
  "private": true,
  "dependencies": {
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "allure-commandline": "^2.13.0",
    "browserstack-local": "^1.4.9",
    "jasmine-allure-reporter": "^1.0.2",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^5.0.2",
    "log4js": "^6.2.1",
    "moment": "^2.25.3",
    "node-vibrant": "^3.1.5",
    "protractor-flake": "^4.0.0",
    "protractor-html-reporter": "^1.3.2",
    "request-promise-native": "^1.0.8",
    "rxjs": "^6.5.5",
    "shortid": "^2.2.15",
    "underscore": "^1.10.2",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.3.3",
    "allure-commandline": "^2.13.0",
    "codelyzer": "~5.2.2",
    "husky": "^1.3.1",
    "jasmine-core": "~3.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-jasmine-rules": "^1.6.1",
    "typescript": "~3.9.2"
  }
}
